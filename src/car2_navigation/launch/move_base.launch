<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find car2_navigation)/config/car2/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find car2_navigation)/config/car2/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find car2_navigation)/config/car2/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find car2_navigation)/config/car2/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find car2_navigation)/config/car2/base_local_planner_params.yaml" command="load" />
  </node>
  
</launch>
